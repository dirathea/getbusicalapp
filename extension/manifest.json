{
  "manifest_version": 3,
  "name": "BusiCal - Share to Calendar",
  "version": "0.1.0",
  "description": "Share Google Calendar events to other calendars with privacy protection. Events sync as 'Synced Event' marked busy.",
  "permissions": ["storage", "activeTab"],
  "host_permissions": ["https://calendar.google.com/*"],
  "content_scripts": [
    {
      "matches": ["https://calendar.google.com/*"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
